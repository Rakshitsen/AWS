{
    "Version": "2012-10-17",
    "Statement":[
        {
            "Effect":"Allow",
            "Action":[
                "s3:GetObject",
                "s3:ListBucket"
            ],
            "Resource":[
                "arn:aws:s3:::my-nginx-content-bucket",
                "arn:aws:s3:::my-nginx-content-bucket/*"
                
            ]
        },
        {
            "Effect":"Allow",
            "Action": "s3:ListAllMyBuckets",
            "Resource":"*"
        }
    ]
}
